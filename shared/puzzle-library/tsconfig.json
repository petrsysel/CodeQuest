{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "declaration": true,
      "composite": true,
      "paths": {
        "@utils":["../utils/src/index"]
      }
    },
    "references": [
      {
        "path": "../utils/"
      }
    ]
}
  